dNetLemmatizer\nfrom nltk import pos_tag\n\n# Define entity types\nentity_types = ['organization', 'person', 'geo', 'event']\n\n# Define a function to extract entities\ndef extract_entities(text, entity_types):\n    # Tokenize the text\n    tokens = word_tokenize(text)\n\n    # Remove stopwords and punctuation\n    stop_words = set(stopwords.words('english'))\n    tokens = [token for token in tokens if token not in stop_words and token.isalpha()]\n\n    # Lemmatize the tokens\n    lemmatizer = WordNetLemmatizer()\n    tokens = [lemmatizer.lemmatize(token) for token in tokens]\n\n    # Tag the tokens with their part-of-speech\n    tagged_tokens = pos_tag(tokens)\n\n    # Initialize a dictionary to store the entities\n    entities = {}\n\n    # Iterate over the tagged tokens\n    for token, tag in tagged_tokens:\n        # Check if the token is a noun\n        if tag in ['NN', 'NNS', 'NNP', 'NNPS']:\n            # Check if the token is a proper noun\n            if token.istitle():\n                # Check if the token is a known entity\n                if token in entities:\n                    # If the token is already in the entities dictionary, append the new information\n                    entities[token]['description'] += ' ' + text\n                else:\n                    # If the token is not in the entities dictionary, add it\n                    entities[token] = {\n                        'name': token,\n                        'type': None,\n                        'description': text\n                    }\n\n    # Map the entity types\n    entity_types_map = {\n        'NNP': 'organization',\n        'NN': 'person',\n        'GPE': 'geo',\n        'EVENT': 'event'\n    }\n\n    # Update the entity types\n    for entity, info in entities.items():\n        info['type'] = entity_types_map.get(info['name'][-1], 'organization')\n\n    # Return the entities\n    return [(entity, info['name'], info['type'], info['description']) for entity, info in entities.